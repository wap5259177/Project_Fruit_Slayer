<div id="datagrid-tab" style='padding-left:10px;padding-top:5px;'>
	<div>
	$!{select}
	</div>	
	<!--<a class="bupabutton" style='padding-left:10px' href="#" data-settings="iconClassname:'icon-excel'"
		 onclick="this.href='index.excel?batch='+jQuery('#sel-batch').val()" target="_blank">导出Excel</a>-->
	<div style="font-size:20px;text-align:center">广东省新农村连片示范建设工程台账簿 - 所有主体行政村情况累计汇总统计</div>
</div>
<table class="bupgrid" fit="true" data-settings="nowrap:false" toolbar="#datagrid-tab" id='mygrid'>
	<!--这一块让它固定不动 frozen:true-->
	<thead data-settings="frozen:true">
		<tr>
		   	<th id="hideth" style="width:60px;" rowspan='3' data-settings="field:'code'">序号</th>
		   	<th id="hideth3" style="width:60px;" rowspan='3' data-settings="field:'code2'">批次</th>
			<th id="hideth2" style="width:60px;" rowspan='3' data-settings="field:'code3'">县(区)</th>
			<th id="hideth3" style="width:60px;" rowspan='3' data-settings="field:'code4'">行政村名</th>
			
			
		</tr>
	</thead>
	<thead>
		<tr>
			<th style='font-weight:bold;' colspan='5'>各年度农民人均纯收入</th>
			<th style="width:160px;" rowspan='3' data-settings="field:'item10'" >主体行政村</th>
		</tr>
		
		<tr>
			<th style="width:160px;" data-settings="field:'item5'">2013年</th>
			<th style="width:160px;" data-settings="field:'item6'">2014年</th>
			<th style="width:160px;" data-settings="field:'item7'">2015年</th>
			<th style="width:160px;" data-settings="field:'item8'">2016年</th>
			<th style="width:160px;" data-settings="field:'item9'">2017年</th>
		</tr>
	</thead>
	<tbody id='_tbody'>$!{content}</tbody>
</table>
